{
    "version": 2,
    "functions": {
        "api/index.ts": {
            "runtime": "nodejs20.x"
        }
    },
    "rewrites": [
        {
            "source": "/events",
            "destination": "/api/index"
        },
        {
            "source": "/vehicles/(.*)",
            "destination": "/api/index"
        },
        {
            "source": "/zones",
            "destination": "/api/index"
        },
        {
            "source": "/health",
            "destination": "/api/index"
        }
    ]
}