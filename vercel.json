{
    "version": 2,
    "functions": {
        "api/index.ts": {
            "runtime": "@vercel/node@3.0.7"
        }
    },
    "rewrites": [
        {
            "source": "/events",
            "destination": "/api/index"
        },
        {
            "source": "/vehicles/(.*)",
            "destination": "/api/index"
        },
        {
            "source": "/zones",
            "destination": "/api/index"
        },
        {
            "source": "/health",
            "destination": "/api/index"
        }
    ],
    "headers": [
        {
            "source": "/public/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ]
}