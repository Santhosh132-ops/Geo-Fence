{
    "version": 2,
    "builds": [
        {
            "src": "api/index.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/events",
            "dest": "/api/index"
        },
        {
            "src": "/vehicles/(.*)",
            "dest": "/api/index"
        },
        {
            "src": "/zones",
            "dest": "/api/index"
        },
        {
            "src": "/health",
            "dest": "/api/index"
        },
        {
            "src": "/(.*\\.(js|css|html|png|jpg|ico))",
            "dest": "/public/$1"
        },
        {
            "src": "/",
            "dest": "/public/index.html"
        }
    ],
    "outputDirectory": "public"
}